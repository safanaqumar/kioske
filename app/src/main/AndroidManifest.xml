<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.planet01.kioske">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Kioske">
        <activity android:name=".Search_result"></activity>
        <activity android:name=".activities.Splash" />
        <activity android:name=".activities.Signup" />
        <activity android:name=".activities.Every_day_essentails" />
        <activity android:name=".activities.Login" />
        <activity android:name=".activities.Product_view" />
        <activity android:name=".activities.OrderAgain" />
        <activity android:name=".activities.Activity_cart_layout" />

        <activity android:name=".activities.CartLayout">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.HotBundles"
            android:theme="@style/Theme.Kioske.NoActionBar"></activity>
        <activity
            android:name=".activities.MainActivity"
            android:theme="@style/Theme.Kioske.NoActionBar"></activity>
    </application>

</manifest>